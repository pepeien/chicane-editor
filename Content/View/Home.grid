<View id="home">
    <Text>{{ getFPS() }} FPS {{ getFrametime() }} ms</Text>
    <List
        id="textureForm"
        width="100vw"
        height="80vh"
        direction="column"
    >
        <Text>Texture Name</Text>
        <TextInput value="name"></TextInput>
        <Button onClick="onSelectImage">Select Image</Button>
        <Text>{{ imagePath }}</Text>
        <Button onClick="onSubmit">Create</Button>
    </List>
</View>